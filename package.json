{
  "name": "twemoji-spritesheet",
  "version": "0.1.1",
  "keywords": [
    "twemoji",
    "emoji-picker",
    "emojis",
    "spritesheet",
    "spritesheet-packer"
  ],
  "repository": "github:leodr/twemoji-spritesheet",
  "license": "MIT",
  "author": "Leonhard Driesch",
  "bin": {
    "twemoji-spritesheet": "dist/index.js"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "format": "prettier . --write --ignore-path .gitignore",
    "dev": "ts-node src/index.ts ./__tests__/emojis-en-v13.0.json --out-file my-output.png",
    "build": "ncc build src/index.ts --minify --out dist",
    "test": "jest",
    "lint": "eslint . --ignore-path .gitignore",
    "check-types": "tsc --noEmit",
    "check-format": "prettier . --ignore-path .gitignore --check",
    "validate": "run-p test lint check-types check-format",
    "prepublishOnly": "yarn validate && yarn build",
    "prepare": "husky install && shx rm -rf .git/hooks && shx ln -s ../.husky .git/hooks"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/mock-fs": "^4.13.0",
    "@types/node-fetch": "^2.5.8",
    "@types/twemoji-parser": "^12.1.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "@vercel/ncc": "^0.27.0",
    "canvas": "^2.7.0",
    "commander": "^7.1.0",
    "eslint": "^7.21.0",
    "eslint-plugin-jest": "^24.2.1",
    "husky": "^5.1.3",
    "image-size": "^0.9.4",
    "jest": "^26.6.3",
    "mock-fs": "^4.13.0",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-plugin-packagejson": "^2.2.10",
    "pretty-quick": "^3.1.0",
    "shx": "^0.3.3",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "twemoji-parser": "^13.0.0",
    "typescript": "^4.2.3",
    "yup": "^0.32.9"
  },
  "engines": {
    "node": ">=12"
  }
}
